{
  "name": "electerm",
  "version": "0.26.40",
  "description": "Terminal/ssh/sftp client(linux, mac, win) based on electron/ssh2/node-pty/xterm/antd and other libs",
  "main": "dev-app.js",
  "bin": "npm/electerm",
  "scripts": {
    "app": "node bin/app",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress",
    "start": "./node_modules/.bin/webpack-dev-server --progress --colors --content-base app/assets/",
    "icon": "node bin/icon",
    "ver": "node bin/generate-version",
    "clean": "node bin/clean",
    "compile": "node bin/build",
    "prepublishOnly": "node npm/prepublish",
    "postpublish": "node npm/postpublish",
    "prepare-file": "node bin/prepare",
    "output": "./node_modules/.bin/electron-builder",
    "build-linux": "node bin/build-linux",
    "release": "npm run pre-test && npm run output",
    "preinstall": "node bin/preinstall",
    "postinstall": "node bin/post-install",
    "lint": "./node_modules/.bin/eslint --max-warnings 0 --ext .js --ext .jsx src app bin test",
    "test": "./node_modules/.bin/mocha test/e2e/*.js",
    "pre-test": "npm run compile && npm run prepare-file"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
