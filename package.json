{
  "name": "electerm",
  "version": "0.7.0",
  "description": "electerm",
  "main": "app.js",
  "scripts": {
    "dev": "cd app && NODE_ENV=development ../node_modules/.bin/electron ./app",
    "gulp": "./node_modules/.bin/gulp",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress",
    "s": "./node_modules/.bin/nodemon --expose-gc --config nodemon.json ./bin/www",
    "c": "./node_modules/.bin/webpack-dev-server --progress --colors --content-base app/assets/",
    "icon": "node bin/icon",
    "ver": "node bin/generate-version.js",
    "clean": "rm -rf app/assets/js && rm -rf app/assets/css && rm -rf app/assets/images && rm -rf app/assets/index.html",
    "pack": "node bin/pack",
    "compile": "node bin/build",
    "prepare-file": "node bin/prepare",
    "dist-test": "npm run compile && npm run prepare-file && npm run pack",
    "output": "./node_modules/.bin/electron-builder",
    "output-cn": "./node_modules/.bin/electron-builder -c local-electron-builder.json",
    "release": "npm run compile && npm run prepare-file && npm run output",
    "preinstall": "rm -rf package-lock.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electerm/electerm.git"
  },
  "author": "ZHAO Xudong <zxdong@gmail.com>",
  "bugs": {
    "url": "https://github.com/electerm/electerm/issues"
  },
  "homepage": "https://github.com/electerm/electerm#readme",
  "engines": {
    "node": ">=8.9.1"
  },
  "devDependencies": {
    "antd": "2.13.10",
