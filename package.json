{
  "name": "electerm",
  "version": "1.14.0",
  "description": "Terminal/ssh/sftp client(linux, mac, win) based on electron/ssh2/node-pty/xterm/antd/subx and other libs",
  "main": "app.js",
  "bin": "npm/electerm",
  "scripts": {
    "app": "node bin/app",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --config build/webpack.config.js",
    "start": "./node_modules/.bin/webpack serve --hot --progress --content-base work/app/assets/ --config build/webpack.config.js",
    "icon": "node bin/icon",
    "ver": "node bin/generate-version",
    "clean": "node bin/clean",
    "compile": "node bin/build",
    "local-test": "LOCAL_TEST=1 npm run test",
    "prepublishOnly": "node npm/prepublish",
    "postpublish": "node npm/postpublish",
    "prepare-file": "node bin/prepare",
    "output": "./node_modules/.bin/electron-builder",
    "build-mac": "node bin/build-mac",
    "build-linux": "node bin/build-linux",
    "build-win": "node bin/build-win",
    "release": "npm run pre-test && npm run output",
    "preinstall": "node bin/preinstall",
    "postinstall": "node bin/post-install",
    "lint": "./node_modules/.bin/standard --verbose | snazzy",
    "fix": "./node_modules/.bin/standard --fix",
    "test": "./node_modules/.bin/mocha test/e2e/*.js",
    "pre-test": "npm run compile && npm run prepare-file"
  },
  "license": "MIT",
  "langugeRepo": "https://github.com/electerm/electerm-locales",
  "privacyNoticeLink": "https://github.com/electerm/electerm/wiki/privacy-notice",
  "knownIssuesLink": "https://github.com/electerm/electerm/wiki/Know-issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electerm/electerm.git"
  },
  "author": {
    "name": "ZHAO Xudong",
    "email": "zxdong@gmail.com",
    "url": "https://github.com/zxdong262"
  },
  "bugs": {
    "url": "https://github.com/electerm/electerm/issues"
  },
  "homepage": "https://electerm.github.io/electerm",
  "engines": {
    "node": ">=12.0.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@ant-design/icons": "4.6.2",
