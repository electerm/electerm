{
  "name": "electerm",
  "version": "0.3.0",
  "description": "electerm",
  "main": "app.js",
  "scripts": {
    "dev": "cd app && NODE_ENV=development ../node_modules/.bin/electron ./app",
    "gulp": "./node_modules/.bin/gulp",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --progress",
    "s": "./node_modules/.bin/nodemon --expose-gc --config nodemon.json ./bin/www",
    "c": "./node_modules/.bin/webpack-dev-server --progress --colors --content-base app/assets/",
    "icon": "node bin/icon",
    "ver": "node bin/generate-version.js",
    "clean": "rm -rf app/assets/js && rm -rf app/assets/css && rm -rf app/assets/images && rm -rf app/assets/index.html",
    "dist": "node bin/dist",
    "bat": "npm run dist",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "preinstall": "rm -rf package-lock.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
